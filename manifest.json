{
    "manifest_version": 3,
    "name": "copart-converter-chrome-extension",
    "description": "A simple chrome extension that converts miles to kilometers on Copart auto auctions",
    "version": "1.0",
    "icons": {
      "16": "images/logo_16.png",
      "48": "images/logo_48.png"
    },
    "action": {
      "default_icon": "images/logo_16.png"
    },
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://www.copart.com/lot/*"
        ]
      }
    ]
}